-- TOC entry 213 (class 1255 OID 18640)
-- Name: deleteitem(integer, integer); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION deleteitem(integer, integer) RETURNS void
    LANGUAGE sql
    AS $_$

update estoque set
quantidade = (quantidade - (select SUM(h.qtde_compra) from historico_compras 
as h where h.id_cliente = $1 and h.id_produto = $2))
where id_produto = $2

$_$;


ALTER FUNCTION public.deleteitem(integer, integer) OWNER TO postgres;

